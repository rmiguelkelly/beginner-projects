#include <stdio.h>
#include <stdlib.h>
//prints out the Fibonacci number at the specified index
int fibonacciSequence(int n);

//main method
int main(int argc, char*args[])
{
    int i;
    for (i = 0; i < 20; i++)
    {
        printf("%d ", fibonacciSequence(i));
    }
    printf("\n");
}

int fibonacciSequence(int n)
{
    if (n < 0)
    {
        return 1;
    }
    else if (n <= 1)
    {
        return n;
    }
    else
    {
        return fibonacciSequence(n - 2) + fibonacciSequence(n - 1);
    }
}
